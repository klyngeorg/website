{
  "name": "website",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
    "format": "prettier --write --plugin prettier-plugin-svelte .",
    "postbuild": "svelte-sitemap --domain https://klyngeorg.no && pnpm generate-seo-images",
    "prepare": "husky install",
    "generate-seo-images": "tsx src/bin/generate-seo-images.ts"
  },
  "lint-staged": {
    "*.{js,ts,css,md,json,svelte}": "prettier --plugin prettier-plugin-svelte --write",
    "*.{js,ts}": "eslint --fix"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.22.3",
    "@types/md5": "^2.3.2",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "schema-dts": "^1.1.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.6",
    "svelte-sitemap": "^2.5.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vite-imagetools": "^5.0.4",
    "vitest": "^0.34.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/outfit": "^5.0.0",
    "@googleapis/calendar": "^9.0.0",
    "@resvg/resvg-js": "^2.4.1",
    "@sanity/client": "^6.0.0",
    "@sanity/image-url": "^1.0.2",
    "@sveltejs/adapter-static": "^2.0.0",
    "convert-svg-to-png": "^0.6.4",
    "date-fns": "^2.29.3",
    "groqd": "^0.15.9",
    "md5": "^2.3.0",
    "puppeteer-core": "^21.0.0",
    "slugify": "^1.6.5",
    "svelte-meta-tags": "^3.0.0",
    "ts-invariant": "^0.10.3"
  }
}
