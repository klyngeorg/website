{
	"name": "website",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@8.4.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"postbuild": "svelte-sitemap --domain https://klyngeorg.no && pnpm generate-seo-images",
		"prepare": "husky install",
		"codegen": "graphql-codegen-esm",
		"generate-seo-images": "ts-node-esm --experimental-specifier-resolution=node src/bin/generate-seo-images.ts"
	},
	"lint-staged": {
		"*.{js,ts,css,md,json,svelte}": "prettier --plugin-search-dir . --write",
		"*.{js,ts}": "eslint --fix"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "3.3.1",
		"@graphql-codegen/client-preset": "2.1.1",
		"@graphql-codegen/introspection": "3.0.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/md5": "^2.3.2",
		"@types/node": "^18",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.0",
		"svelte-sitemap": "^2.5.4",
		"svimg": "^3.1.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.0.0",
		"vitest": "^0.30.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^4.5.10",
		"@googleapis/calendar": "^6.0.0",
		"@graphql-typed-document-node/core": "^3.1.1",
		"@resvg/resvg-js": "^2.4.1",
		"@sanity/client": "^5.0.0",
		"@sanity/image-url": "^1.0.2",
		"@sveltejs/adapter-static": "^1.0.0",
		"convert-svg-to-png": "^0.6.4",
		"date-fns": "^2.29.3",
		"graphql": "^16.6.0",
		"graphql-request": "^6.0.0",
		"md5": "^2.3.0",
		"puppeteer-core": "^19.7.2",
		"schema-dts": "^1.1.0",
		"slugify": "^1.6.5",
		"ts-invariant": "^0.10.3"
	}
}
